<div class="row">
  <div class="span6 offset3">
    <%= form_for(@subject) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.submit button_label, class: "btn btn-primary" %>
      <%= link_to "Cancel", subjects_path, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

